# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

#GET		/home						controllers.Application.home()
#GET		/about						controllers.Application.about()
GET		/home						controllers.Application.home()
GET		/about						controllers.Application.about()
GET		/cafe						controllers.Application.cafe()
GET		/contacts					controllers.Application.contacts()
GET		/giftShop					controllers.Application.giftShop()
GET		/salon						controllers.Application.salon()
GET		/spa						controllers.Application.spa()
GET		/specialEvents				controllers.Application.specialEvents()
GET		/teaRoom					controllers.Application.teaRoom()
GET		/viewCart					controllers.Application.viewCart()